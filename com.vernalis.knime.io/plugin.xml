<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

   <!-- This is an example for providing an own category 
      <extension
         point="org.knime.workbench.repository.categories">
      <category
            description="Category containing my nodes"
            icon="icons/default.png"
            level-id="myNodes"
            name="My nodes"
            path="/"/>
   </extension>
   -->
   
   <extension
         point="org.knime.workbench.repository.nodes">
      <node
            category-path="/community/vernalis/io"
            factory-class="com.vernalis.knime.io.nodes.load.txt.LoadTextFilesNodeFactory" id="1"/>
      <node
            category-path="/community/vernalis/io"
            factory-class="com.vernalis.knime.io.nodes.load.pdb.LoadLocalPdbFilesNodeFactory" id="2">
      </node>
      <node
            category-path="/community/vernalis/io"
            factory-class="com.vernalis.knime.io.nodes.load.cdxml.LoadLocalCdxmlFilesNodeFactory" id="3">
      </node>
      <node
            category-path="/community/vernalis/io"
            factory-class="com.vernalis.knime.io.nodes.load.xml.LoadLocalXmlFilesNodeFactory" id="4">
      </node>
      <node
            category-path="/community/vernalis/io"
            factory-class="com.vernalis.knime.io.nodes.load.mol.LoadLocalMolFilesNodeFactory" id="5">
      </node>
      <node
            category-path="/community/vernalis/io"
            factory-class="com.vernalis.knime.io.nodes.load.mol2.LoadLocalMol2FilesNodeFactory" id="6">
      </node>
      <node
            category-path="/community/vernalis/io"
            factory-class="com.vernalis.knime.io.nodes.load.rxn.LoadLocalRxnFilesNodeFactory" id="7">
      </node>
   </extension>
   <extension
         point="org.knime.workbench.repository.categories">
      <category
            icon="icons/IO_icon.png"
            level-id="io"
            name="IO"
            path="/community/vernalis/">
      </category>
   </extension>
   
</plugin>
