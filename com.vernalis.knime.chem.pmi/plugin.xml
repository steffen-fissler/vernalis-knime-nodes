       category-path="/community/vernalis/chem/pmi/plot"
       deprecated="false"
       factory-class="com.vernalis.knime.chem.pmi.nodes.plot.kerneldensity.PMIKernelDensityPlotNodeFactory">
 </node>
</extension>
<extension point="org.knime.workbench.repository.categories">
	<category description="Nodes relating to PMI plotting"
		icon="icons/PMI_icon.png"
		level-id="pmi"
		name="Principal Moments of Inertia (PMIs)"
		path="/community/vernalis/chem/">
	</category>
 <category
       icon="icons/PMI_icon2.png"
       level-id="plot"
       locked="true"
       name="PMI Plotting"
       path="/community/vernalis/chem/pmi/">
 </category>
</extension>   
</plugin>
