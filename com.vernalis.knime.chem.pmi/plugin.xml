<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?eclipse version="3.0"?>
<plugin>
<extension point="org.knime.workbench.repository.nodes">
	<node
       category-path="/community/vernalis/chem/pmi"
       deprecated="true"
       factory-class="com.vernalis.knime.chem.pmi.nodes.pmi2.Pmi2NodeFactory"/>
	<node category-path="/community/vernalis/chem/pmi"
		factory-class="com.vernalis.knime.chem.pmi.nodes.transform.toprincaxes.AlignToPrincipalAxesNodeFactory"
		/>
 <node
       category-path="/community/vernalis/chem//pmi"
       deprecated="false"
       factory-class="com.vernalis.knime.chem.pmi.nodes.props.pmiderived.PmiDerivedPropsNodeFactory">
 </node>
 <node
       category-path="/community/vernalis/chem/pmi/plot"
       deprecated="false"
       factory-class="com.vernalis.knime.chem.pmi.nodes.plot.scatter.PMIScatterPlotNodeFactory">
 </node>
 <node
       category-path="/community/vernalis/chem/pmi/plot"
       deprecated="false"
       factory-class="com.vernalis.knime.chem.pmi.nodes.plot.convexhull.PMIHullPlotNodeFactory">
 </node>
