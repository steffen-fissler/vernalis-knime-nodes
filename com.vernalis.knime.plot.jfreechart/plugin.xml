<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

   <!-- This is an example for providing an own category 
      <extension
         point="org.knime.workbench.repository.categories">
      <category
            description="Category containing my nodes"
            icon="icons/default.png"
            level-id="myNodes"
            name="My nodes"
            path="/"/>
   </extension>
   -->
   
   <extension
         point="org.knime.workbench.repository.nodes">
      <node
            category-path="community/vernalis/plot/kernel"
            deprecated="false"
            factory-class="com.vernalis.knime.plot.nodes.kerneldensity.KernelDensity1DPlotNodeFactory">
      </node>
      <node
            category-path="community/vernalis/plot/kernel"
            deprecated="false"
            factory-class="com.vernalis.knime.plot.nodes.kerneldensity.KernelDensity2DPlotNodeFactory">
      </node>
      <node
            category-path="community/vernalis/plot/kernel"
            deprecated="false"
            factory-class="com.vernalis.knime.plot.nodes.kerneldensity.KernelLoopStartNodeFactory">
      </node>
      <node
            category-path="community/vernalis/plot"
            deprecated="false"
            factory-class="com.vernalis.knime.plot.nodes.box.BoxplotNodeFactory">
      </node>
   </extension>
   <extension
         point="org.knime.workbench.repository.categories">
      <category
            icon="icons/kernel_contour2D.png"
            level-id="kernel"
            locked="true"
            name="Kernel Density"
            path="community/vernalis/plot/">
      </category>
   </extension>
 
   
</plugin>
