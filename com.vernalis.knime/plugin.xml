<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         point="org.knime.workbench.repository.nodes">
      <node
            category-path="/community/vernalis/rcsb"
            factory-class="com.vernalis.pdbconnector.PdbConnectorNodeFactory"
            id="PdbConnectorNodeFactory">
      </node>
      <node
            category-path="/community/vernalis/local.pdb"
            expert-flag="false"
            factory-class="com.vernalis.nodes.io.pdb.loadlocal.LocalPDBLoadNodeFactory2"
            id="LocalPDBLoadNodeFactory2">
      </node>
      <node
            after="LocalPDBLoadNodeFactory2"
            category-path="/community/vernalis/local.pdb"
            expert-flag="false"
            factory-class="com.vernalis.nodes.io.pdb.savelocal.SavePDBLocalNodeFactory"
            id="SavePDBLocalNodeFactory">
      </node>
      <node
            category-path="/community/vernalis/rcsb"
            expert-flag="false"
            factory-class="com.vernalis.nodes.io.rcsb.source.RCSBsDownloadNodeFactory"
            id="RCSBsDownloadNodeFactory">
      </node>
      <node
            after="RCSBsDownloadNodeFactory"
            category-path="/community/vernalis/rcsb"
            expert-flag="false"
            factory-class="com.vernalis.nodes.io.rcsb.manip.RCSBmultiDownloadNodeFactory"
            id="RCSBmultiDownloadNodeFactory">
      </node>
      <node
            category-path="/community/vernalis/sequence"
            expert-flag="false"
            factory-class="com.vernalis.nodes.fasta.FASTASequenceNodeFactory_2"
            id="FASTASequenceNodeFactory_2">
      </node>
      <node
            after="SavePDBLocalNodeFactory"
            category-path="/community/vernalis/local.pdb"
            expert-flag="false"
            factory-class="com.vernalis.nodes.pdb.props.PdbPropertiesNodeFactory"
            id="PdbPropertiesNodeFactory">
      </node>
      <node
            category-path="/community/vernalis/misc"
            expert-flag="false"
            factory-class="com.vernalis.nodes.smartsviewer.SmartsViewer2NodeFactory"
            id="SmartsViewer2NodeFactory">
      </node>
      <node
            category-path="/community/vernalis/misc"
            expert-flag="false"
            factory-class="com.vernalis.nodes.misc.randomnos.RandomNumbersNodeFactory"
            id="RandomNumbersNodeFactory">
      </node>
      <node
            category-path="/community/vernalis/misc"
            expert-flag="false"
            factory-class="com.vernalis.nodes.io.listdirs2.ListDirs2NodeFactory"
            id="ListDirs2NodeFactory">
      </node>
      <node
            category-path="/community/vernalis/misc/txt.IO"
            expert-flag="false"
            factory-class="com.vernalis.nodes.io.txt.LoadTxtNodeFactory"
            id="LoadTxtNodeFactory">
      </node>
      <node
            category-path="/community/vernalis/misc/txt.IO"
            expert-flag="false"
            factory-class="com.vernalis.nodes.io.txt.SaveTxtLocalNodeFactory"
            id="SaveTxtLocalNodeFactory">
      </node>
      <node
            category-path="/community/vernalis/ePMC"
            expert-flag="false"
            factory-class="com.vernalis.nodes.epmc.EuroPmcAdvancedSearchNodeFactory"
            id="EuroPmcAdvancedSearchNodeFactory">
      </node>
      <node
            category-path="/community/vernalis/flowcontrol/fv"
            expert-flag="false"
            factory-class="com.vernalis.flowcontrol.nodes.fv.ifswitch.FVarIfSwitchNodeFactory"
            id="FVarIfSwitchNodeFactory">
      </node>
      <node
            after="FVarIfSwitchNodeFactory"
            category-path="/community/vernalis/flowcontrol/fv"
            expert-flag="false"
            factory-class="com.vernalis.flowcontrol.nodes.fv.varvalifswitch.FlowVarFvvalIfSwitchNodeFactory"
            id="FlowVarFvvalIfSwitchNodeFactory">
      </node>
      <node
            after="FlowVarFvvalIfSwitchNodeFactory"
            category-path="/community/vernalis/flowcontrol/fv"
            expert-flag="false"
            factory-class="com.vernalis.flowcontrol.nodes.fv.endif.FlowVarEndIfNodeFactory"
            id="FlowVarEndIfNodeFactory">
      </node>
      <node
            after="FlowVarEndIfNodeFactory"
            category-path="/community/vernalis/flowcontrol/fv"
            expert-flag="false"
            factory-class="com.vernalis.flowcontrol.nodes.fv.caseselect.FVarFVCaseSelectNodeFactory"
            id="FVarFVCaseSelectNodeFactory">
      </node>
      <node
            after="FVarFVCaseSelectNodeFactory"
            category-path="/community/vernalis/flowcontrol/fv"
            expert-flag="false"
            factory-class="com.vernalis.flowcontrol.nodes.fv.endcase.FlowVarEndCaseNodeFactory"
            id="FlowVarEndCaseNodeFactory">
      </node>
      <node
            category-path="/community/vernalis/sequence"
            expert-flag="false"
            factory-class="com.vernalis.nodes.pdb.getsequence.Pdb2SequenceNodeFactory"
            id="Pdb2SequenceNodeFactory">
      </node>
      <node
            category-path="/community/vernalis/fps"
            expert-flag="false"
            factory-class="com.vernalis.nodes.fingerprint.properties.FpPropsNodeFactory"
            id="FpPropsNodeFactory">
      </node>
      <node
            after="PdbConnectorNodeFactory"
            category-path="/community/vernalis/rcsb"
            factory-class="com.vernalis.pdbconnector.PdbConnectorXmlQueryNodeFactory"
            id="PdbConnectorXmlQueryNodeFactory">
      </node>
   </extension>
   <extension
         point="org.knime.workbench.repository.categories">
      <category
            description="Custom nodes developed by Vernalis (R&amp;D)"
            icon="icons/Vernalis.ico"
            level-id="vernalis"
            name="Vernalis"
            path="/community/">
      </category>
      <category
            description="Vernalis Nodes for RCSB query and reporting"
            icon="icons/RCSB_PDB.png"
            level-id="rcsb"
            name="RCSB PDB Tools"
            path="/community/vernalis/">
      </category>
      <category
            after="local.pdb"
            description="Vernalis nodes for Sequence manipulation tools"
            icon="icons/SeqTools.png"
            level-id="sequence"
            name="Sequence Tools"
            path="/community/vernalis/">
      </category>
      <category
            after="rcsb"
            description="Vernalis file reader nodes"
            icon="icons/IO_icon.png"
            level-id="local.pdb"
            name="Local PDB Tools"
            path="/community/vernalis/">
      </category>
      <category
            after="fps"
            description="Miscellaneous Vernalis nodes"
            icon="icons/missing.gif"
            level-id="misc"
            name="Miscellaneous"
            path="/community/vernalis/">
      </category>
      <category
            description="Text-based file IO Nodes"
            icon="icons/IO_icon.png"
            level-id="txt.IO"
            locked="true"
            name="Text-based IO"
            path="/community/vernalis/misc/">
      </category>
      <category
            icon="icons/ePMCv1.png"
            level-id="ePMC"
            locked="true"
            name="European PubMed Central"
            path="/community/vernalis/">
      </category>
      <category
            description="Flow control nodes"
            icon="icons/FlowControlGroup_16x16.png"
            level-id="flowcontrol"
            locked="true"
            name="Flow Control"
            path="/community/vernalis/">
      </category>
      <category
            icon="icons/FlowControlGroupFlowVars_16x16.png"
            level-id="fv"
            locked="true"
            name="Flow Variable Flow Control"
            path="/community/vernalis/flowcontrol/">
      </category>
      <category
            after="sequence"
            icon="icons/fingerprint_category.png"
            level-id="fps"
            locked="true"
            name="Fingerprints"
            path="/community/vernalis/">
      </category>
   </extension>
   <extension
         point="org.knime.product.splashExtension">
      <splashExtension
            icon="icons/Vernalis_splash_48.png"
            icon24="icons/Vernalis_splash_24.png"
            icon32="icons/Vernalis_splash_32.png"
            id="Vernalis_splashExtension"
            tooltip="Vernalis nodes">
      </splashExtension>
   </extension>
</plugin>
